{
    "name": "gessq",
    "displayName": "gessQ",
    "description": "gess.Q Support",
    "version": "0.2.4",
    "publisher": "volkerdobler",
    "engines": {
        "vscode": "^1.45.1"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "activationEvents": [
        "onLanguage:gessq"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/volkerdobler/gessQ.git"
    },
    "license": "AFL-3.0",
    "main": "./out/extension",
    "icon": "images/gessQ_icon.png",
    "contributes": {
        "languages": [
            {
                "id": "gessq",
                "aliases": [
                    "gessQ",
                    "gessq"
                ],
                "extensions": [
                    ".q"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "gessq",
                "scopeName": "source.gessQ",
                "path": "./syntaxes/gessq.tmLanguage"
            }
        ],
        "snippets": [
            {
                "language": "gessq",
                "path": "./snippets/snippets.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^12.12.47",
        "@types/vscode": "^1.45.1",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-prettier": "^3.1.3",
        "prettier": "^1.19.1",
        "typescript": "^3.9.5"
    },
    "dependencies": {},
    "__metadata": {
        "id": "8369ddfc-3ff3-4da4-8712-89df76ef2c89",
        "publisherId": "7c9d6a5c-7f0a-4217-8a96-4f84eae98522",
        "publisherDisplayName": "Volker Dobler"
    }
}
